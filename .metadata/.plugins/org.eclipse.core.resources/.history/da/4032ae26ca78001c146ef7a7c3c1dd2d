package projetos.todo_list;

public class Util {

	
	public static String formatarTarefa(Task trf) {
		String formT;
		formT = "______________________\n" +  trf.description + "\n Priority: "+ trf.priority+"Task created in: " + trf.calculateTaskAge() + " Its done? " + trf.done; 
		return formT;
	}
}
