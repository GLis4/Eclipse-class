package projetos.hackerankdes;

import java.text.NumberFormat;
import java.util.Locale;
import java.util.Scanner;

public class StaticRadio {
	

	public static void main(String[] args) {
		// staticRadio();
		numberFormat();
	}

	static public void staticRadio() {
		 Scanner sc = new Scanner(System.in);
		 int  b = sc.nextInt();
		 int  a = sc.nextInt();
		if(b<= 0 || a <= 0) {
			System.out.println("Breadth and height must be positive");
		}else {
			int area=b*a;
			System.out.print(area);
		}
	}

	static public void numberFormat(){

		NumberFormat ch = NumberFormat.getInstance(Locale.CHINA);
		System.out.println(ch.format(12324.134));


	}
}
