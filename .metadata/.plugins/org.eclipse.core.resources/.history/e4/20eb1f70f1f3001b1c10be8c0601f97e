package projetos.hackerankdes;

import java.util.Scanner;

public class DesafiosHR {
	public static void main(String[] args) {
		/*java 100
		cpp 65
		python 50
*/
		
		Scanner sc = new Scanner(System.in);
		
		
		String java = sc.next();
		int numOne = sc.nextInt();;
		String cop = sc.next();
		int numTwo = sc.nextInt();;
		String python =sc.next();
		int numThree = sc.nextInt();
		
		System.out.println("================================");
		System.out.println(java +"         " + numOne);
		System.out.println(cop  +"          " + numTwo);
		System.out.println(python+"       " + numThree);
		System.out.println("================================");

	}
}