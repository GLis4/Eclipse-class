package aulas_assunto;

public class variaveisPrimitivos {
	
	public static void main(String[] args) {
		
		byte vByte = 120;
		int vInt = 5;
		double vDouble = 3.35;
		char vChar = '#';
		
		System.out.print("Char     : " + vChar+
						 "\nByte   : " + vByte +
						 "\nDouble: " + vDouble +
						 "\nInt   : " + vInt);
		
		
		System.out.print(vChar + vByte + vDouble/vInt);
		  
	}
	
	
}
