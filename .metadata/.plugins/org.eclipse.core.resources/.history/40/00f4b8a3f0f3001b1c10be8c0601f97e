package projetos.hackerankdes;

public class DesafiosHR {
	public static void main(String[] args) {
		/*java 100
		cpp 65
		python 50
*/
		
		String java = "java";
		int numOne = 100;
		String cop = "cop";
		int numTwo = 065;
		String python = "python";
		int numThree = 050;
		/*================================
		  java           100 
		  cpp            065 
		  python         050 
================================*/
		System.out.println("================================");
		System.out.println(java +"         " + numOne);
		System.out.println(cop  +"          " + numTwo);
		System.out.println(python+"        " + numThree);
		System.out.println("================================");

	}
}