public class GerenciadorIngredientes {

	static List<Ingredients> listIng = new ArrayList<>();
	public static void main(String[] args) {
		comprarEstoque();
		PesquisarIngredientes();
	}

	static void comprarEstoque(){

		
		int qtd = 2;

		for( int i = 0; i < qtd; i++) {
			Ingredients ing = new Ingredients();
			ing.nome = "Coca-Cola";
			ing.qtdEstoque = 1.5;

			listIng.add(ing);
		}
	}

	static void PesquisarIngredientes() {


	}
}
