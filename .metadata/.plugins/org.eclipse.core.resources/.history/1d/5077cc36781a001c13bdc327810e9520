package projetos.hackerankdes;

import java.util.Scanner;

public class ReadFile {
	public static void main(String[] args) {

		Scanner sc = new Scanner (System.in);
        String n = sc.next();
         
         while(sc.hasNext()){
        	 System.out.println(n.lines() + n);
         }
        sc.close();
		
	}
}
